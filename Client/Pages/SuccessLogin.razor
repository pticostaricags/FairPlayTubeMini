@page "/SuccessLogin"
@attribute [Authorize]

<h3>SuccessLogin</h3>

<LoadingIndicator IsLoading="this.IsBusy"></LoadingIndicator>

<AuthorizeView>
    <Authorized>
        <ul class="list-group">
            @foreach (var singleClaim in context.User.Claims)
            {
                <li class="list-group-item">@singleClaim.Type - @singleClaim.Value</li>
            }
        </ul>
    </Authorized>
</AuthorizeView>
