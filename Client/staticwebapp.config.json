{
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "routes": [
    {
      "route": "/.auth/login/twitter",
      "statusCode": 404
    },
    {
      "route": "/.auth/login/aad",
      "statusCode": 404
    },
    {
      "route": "/data-api/rest/ApplicationUser",
      "allowedRoles": [ "admin" ]
    },
    {
      "route": "/api/IndexVideo",
      "allowedRoles": [ "creator" ]
    },
    {
      "route": "/User/UploadVideo",
      "allowedRoles": [ "creator" ]
    }
  ],
  "responseOverrides": {
    "401": {
      "statusCode": 302,
      "redirect": "/.auth/login/github"
    }
  }
}